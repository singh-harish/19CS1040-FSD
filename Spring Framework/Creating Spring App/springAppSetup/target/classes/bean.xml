    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">  
  
	<bean id="studentbean" class="springAppSetup.Student">  
		<property name="name" value="Harish Singh"></property> 
		<property name="regNo" value="1040"></property> 
		<property name="department" value="Computer Science and Engineering"></property> 
	</bean>  
</beans>
